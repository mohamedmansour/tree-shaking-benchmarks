{
    "$schema": "https://json.schemastore.org/swcrc",
    "jsc": {
      "parser": {
        "syntax": "typescript",
        "decorators": true,
        "tsx": true,
        "dynamicImport": true
       },
      "transform": {
        "legacyDecorator": true,
        "useDefineForClassFields": false
      },
      "target": "esnext",
      "externalHelpers": true
    },
    "module": {
      "type": "nodenext"
    },
    "sourceMaps": true,
    // "minify": true,
    "isModule": true
  }